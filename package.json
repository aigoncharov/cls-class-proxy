{
  "name": "cls-class-proxy",
  "version": "0.1.0",
  "description": "Class proxy that adds CLS (continuation-local-storage) context to constructor, method calls, getters and setters",
  "main": "dist/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --recursive --use_strict --require ts-node/register './**/*.test.ts'",
    "compile": "rm -rf ./dist && ./node_modules/.bin/tsc -p ./tsconfig.json",
    "lint": "./node_modules/.bin/tsc -p ./tsconfig.json --noEmit && ./node_modules/.bin/tslint -c ./tslint.json -p ./tsconfig.json",
    "docs": "rm -rf docs/api && ./node_modules/.bin/typedoc --tsconfig ./tsconfig.docs.json --mode file --ignoreCompilerErrors --excludePrivate --excludeProtected --readme none --theme markdown --mdHideSources --mdEngine github --out ./docs/api ./index.ts && find ./docs/api/ -type f -exec sed -i \"s/\\`\\`\\`js/\\\\n\\`\\`\\`js/g\" {} \\;",
    "prepack": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keenondrums/cls-class-proxy.git"
  },
  "author": "keenondrums (andrey.goncharov+it@protonmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keenondrums/cls-class-proxy/issues"
  },
  "homepage": "https://github.com/keenondrums/cls-class-proxy#readme",
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/cls-hooked": "^4.2.1",
    "@types/mocha": "^5.2.5",
    "@types/sinon": "^5.0.5",
    "chai": "^4.2.0",
    "cls-hooked": "^4.2.2",
    "doctoc": "^1.3.1",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "mocha": "^5.2.0",
    "npm-check-updates": "^2.14.2",
    "prettier": "^1.14.3",
    "sinon": "^7.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.13.0",
    "typedoc-plugin-markdown": "git+https://git@github.com/keenondrums/typedoc-plugin-markdown.git",
    "typescript": "^3.1.3"
  },
  "peerDependencies": {
    "cls-hooked": "^4.2.2"
  }
}
